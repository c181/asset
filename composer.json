{
    "name": "c181/asset",
    "description": "CakePHP 3.* Plugin fot Asset css & js",
    "keywords": ["CakePHP", "Asset"],
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "3.4.*"
    },
    "authors": [
        {
            "name": "C181",
            "email": "connetcor181@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "<6.0"
    },
    "autoload": {
        "psr-4": {
            "Asset\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asset\\Test\\": "tests"
        }
    },
    "version": "1.0.0",
    "extra": {
        "installer-name": "Asset"
    },
    "prefer-stable": true
}
