{
    "name": "c181/asset",
    "description": "CakePHP 3.* Plugin fot Asset css & js",
    "type": "cakephp-plugin",
    "require": {
        "cakephp/cakephp": ">=3.3.2 <4.0.0",
        "php": ">=5.5.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "C181",
            "email": "connetcor181@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Asset\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asset\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
